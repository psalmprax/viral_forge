# ettametta: Infrastructure Configuration Template

# --- App Core ---
APP_NAME="ettametta PRO"
ENV="production"
DEBUG=false
SECRET_KEY="generate_a_secure_random_string_here"
PRODUCTION_DOMAIN="https://your-domain.com"
PORT=8000

# --- Internal Service Auth ---
INTERNAL_API_TOKEN="generate_a_secure_random_token_for_service_communication"

# --- Payment Processing ---
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# --- Database & Cache ---
DATABASE_URL="postgresql://user:password@localhost/ettametta"
REDIS_URL="redis://localhost:6379/0"

# --- AI Intelligence (Neural Sentinel) ---
GROQ_API_KEY="gsk_xxxxxxxxxxxxxxxxxxxx"
OPENAI_API_KEY="sk-xxxxxxxxxxxxxxxxxxxx"
USE_OS_MODELS=true

# --- Neural Asset Hub (Audio/Visual) ---
ELEVENLABS_API_KEY="xxxxxxxxxxxxxxxxxxxxxxxxxxxx"
PEXELS_API_KEY="xxxxxxxxxxxxxxxxxxxxxxxxxxxx"
PIXABAY_API_KEY="xxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# --- Social OAuth (Transmission Matrix) ---
# Google/YouTube
GOOGLE_CLIENT_ID="xxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="GOCSPX-xxxxxxxxxxxxxxxxxxxx"
YOUTUBE_API_KEY="AIzaxxxxxxxxxxxxxxxxxxxx"

# TikTok
TIKTOK_CLIENT_KEY="xxxxxxxxxxxxxxxxxxxx"
TIKTOK_CLIENT_SECRET="xxxxxxxxxxxxxxxxxxxx"
TIKTOK_API_KEY="xxxxxxxxxxxxxxxxxxxx"

# --- Persistence Layer (AWS S3) ---
AWS_ACCESS_KEY_ID="AKIAxxxxxxxxxxxxxxxx"
AWS_SECRET_ACCESS_KEY="xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
AWS_REGION="us-east-1"
AWS_STORAGE_BUCKET_NAME="viral-forge-vault"

# --- Commerce (The Harvest) ---
# Note: These can also be set via the Dashboard Settings UI
SHOPIFY_SHOP_URL="your-store.myshopify.com"
SHOPIFY_ACCESS_TOKEN="shpat_xxxxxxxxxxxxxxxxxxxx"

# --- Video Engine Studio ---
VF_FONT_PATH="/usr/share/fonts/truetype/dejavu/DejaVuSans-Bold.ttf"
USE_GPU=true
